<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="sparrow.com.android.khosbayar.sparrowv10">

    <application
        android:allowBackup="true"
        android:hardwareAccelerated="false"
        android:icon="@drawable/main_logo"
        android:label="@string/app_name"
        android:largeHeap="true"
        android:roundIcon="@drawable/main_logo"
        android:supportsRtl="true">
        <activity
            android:name=".MainActivity"
            android:theme="@style/SplashTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />

            </intent-filter>
        </activity>
        <activity
            android:name=".MenuActivity"
            android:theme="@style/AppTheme"></activity>
        <activity
            android:name=".IntroActivity"
            android:theme="@style/AppTheme"></activity>
        <activity
            android:name=".SizeActivity"
            android:theme="@style/AppTheme"></activity>
        <activity
            android:name=".Level1Activity"
            android:theme="@style/AppTheme"></activity>
        <activity
            android:name=".Level2Activity"
            android:theme="@style/AppTheme"></activity>
        <activity
            android:name=".Level3Activity"
            android:theme="@style/AppTheme"></activity>
        <activity
            android:name=".NextmissionActivity"
            android:theme="@style/AppTheme"></activity>
        <activity
            android:name=".TryagainActivity"
            android:theme="@style/AppTheme"></activity>
        <activity
            android:name=".Dashboard1Activity"
            android:theme="@style/AppTheme"></activity>

        <activity
            android:name=".Dashboard2Activity"
            android:theme="@style/AppTheme"></activity>

        <activity
            android:name=".Dashboard3Activity"
            android:theme="@style/AppTheme"></activity>
    </application>

</manifest>